{
  "name": "locations",
  "version": "1.0.0",
  "description": "locations app using type orm",
  "main": "app.js",
  "scripts": {
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js -f ./ormconfig.json",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n addActive2ColumnToLocationsTable -p",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "ts-node-dev --respawn ./src/app.ts"
  },
  "author": "Mustafa Elsherbiny",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.13",
    "@types/multer": "^1.4.11",
    "@types/node": "^17.0.21",
    "ts-node": "^10.6.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.14.0",
    "flat": "^6.0.1",
    "helmet": "^5.0.2",
    "joi": "^17.11.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.44"
  }
}
